\mode<presentation>

% Set theme colors
\setbeamercolor*{normal text}{fg=white}
\setbeamercolor*{structure}{fg=white}

\setbeamercolor{title page header}{fg=ColorOne!65!black}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{institute}{fg=white}
\setbeamercolor*{date}{fg=white}

\setbeamercolor*{item}{fg=ColorTwo}
\setbeamercolor*{alerted text}{fg=ColorTwo}

\setbeamercolor*{title in head/foot}{fg=ColorOne!45!black, bg=ColorOne!25!black!25}
\setbeamercolor*{frame title}{fg=ColorOne}

% Items
\setbeamertemplate{itemize item}{$\bullet$}

% Sections inToC
\setbeamertemplate{section in toc}{\leavevmode \bf \inserttocsection\par}
\setbeamertemplate{subsection in toc}{\leavevmode\leftskip=0em$\bullet$\hskip1em\inserttocsubsection\par}

\mode
<all>